<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--DefaultColors-->

    <Color x:Key="Background.Black">#FF0A1214</Color>
    <Color x:Key="Background.ColorFill">#FF153726</Color>
    <Color x:Key="Background.MouseOver">#FF196363</Color>

    <Color x:Key="Foreground">#FF8EE4B2</Color>
    <Color x:Key="Foreground.Border">#FF335D5D</Color>
    
