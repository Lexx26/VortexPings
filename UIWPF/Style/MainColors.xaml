<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--DefaultColors-->

    <Color x:Key="BackgroundColor.Black">#FF0A1214</Color>
    <Color x:Key="BackgroundColor.ColorFill">#FF153726</Color>
    <Color x:Key="BackgroundColor.MouseOver">#FF196363</Color>

    <Color x:Key="ForegroundColor">#FF8EE4B2</Color>
    <Color x:Key="ForegroundColor.Border">#FF335D5D</Color>
    
    <Color x:Key="DisabledColor.Background">#FF292929</Color>
    <Color x:Key="DisabledColor.Border">#FF3C4040</Color>
    <Color x:Key="DisabledColor.Foreground">#FF555555</Color>

    <Color x:Key="RedColor">#FFAA3B3B</Color>

</ResourceDictionary>